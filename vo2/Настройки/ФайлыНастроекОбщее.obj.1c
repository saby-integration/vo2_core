//Функция определяет ини на выгрузку
&НаКлиенте
Функция Ини_Выгрузка(Кэш, ДокументСсылка, ДокументПредставление, СбисРеестр) Экспорт
	Если ЗначениеЗаполнено(ДокументПредставление) Тогда
		ИниДокумента = ДокументПредставление;
	Иначе
		ИмяДокумента = Кэш.ОбщиеФункции.ПолучитьРеквизитМетаданныхОбъекта(ДокументСсылка, "Имя");
		ИниДокумента = ИмяДокумента;
		Если	ЗначениеЗаполнено(СбисРеестр)
			И	ЗначениеЗаполнено(СбисРеестр.ТипДок) Тогда
			ИниРеестра = Кэш.ФормаНастроек.Ини(Кэш, СбисРеестр.ТипДок);
			Если ИниРеестра.Свойство(ИниДокумента) Тогда
				ИниДокумента = Кэш.ОбщиеФункции.РассчитатьЗначение(ИмяДокумента, ИниРеестра, Кэш);
			КонецЕсли;
		КонецЕсли;
	КонецЕсли;
	Возврат ИниДокумента;
КонецФункции

#Область Интерфейс  
#Область ОбработчикиСобытийФормы

&НаКлиенте
Процедура сбисПереключитьКонфигурацию(СтруктураКонфигурации, Кэш) Экспорт
	
	ДоступныеИниФайлы		= Кэш.ФормаНастроекОбщее.СформироватьМассивТабДоступныеИниФайлыОбновить(СтруктураКонфигурации, ТабУстановленныеИниФайлы);
	ОбновитьПоляФормы(Новый Структура("ТабДоступныеИниФайлы", ДоступныеИниФайлы));
	
КонецПроцедуры

//Процедура записывает в местный и глобальный кэш измененный параметр	
&НаКлиенте
Процедура ПараметрыПриИзменении(Элемент) Экспорт
	
	ПутьКДаннымФормы			= Сред(Элемент.Имя, Найти(Элемент.Имя, "_") + 1);
	МестныйКэш.Парам[ПутьКДаннымФормы] = ЭтаФорма[ПутьКДаннымФормы]; 
	
КонецПроцедуры

&НаКлиенте
Процедура СпособХраненияНастроекПриИзменении(Элемент)
	
	// При изменении варианта хранения настроек	"перезапускаем" обработку
	ИзмениласьФормаНастроек = (МестныйКэш.Парам.СпособХраненияНастроек <> СпособХраненияНастроек);
	Если	СпособХраненияНастроек	= 0				Тогда
		
		ДиалогОткрытия = Новый ДиалогВыбораФайла(РежимДиалогаВыбораФайла.ВыборКаталога); 
		ДиалогОткрытия.Каталог = КаталогНастроек;
		ДиалогОткрытия.Заголовок = "Выберите каталог"; 
		
		Если ДиалогОткрытия.Выбрать() Тогда 
			
			КаталогНастроек = ДиалогОткрытия.Каталог;
			ПараметрыПриИзменении(МестныйКэш.ГлавноеОкно.сбисЭлементФормы(ЭтаФорма, "КаталогНастроек"));
			
		Иначе//Вернуть как было
			
			СпособХраненияНастроек	= МестныйКэш.Парам.СпособХраненияНастроек;
			Возврат;
			
		КонецЕсли; 
		
		ПараметрыПриИзменении(Элемент);
		ПараметрыИнтеграции = Новый Структура("АдресСервера, СпособОбмена, СпособХраненияНастроек, ВремяОжиданияОтвета", МестныйКэш.СБИС.АдресСервера);
		ЗаполнитьЗначенияСвойств(ПараметрыИнтеграции, МестныйКэш.Парам);
		МестныйКэш.ГлавноеОкно.ПерезапуститьГлавноеОкно(ПараметрыИнтеграции, Ложь, ИзмениласьФормаНастроек);
		
	ИначеЕсли	СпособХраненияНастроек	= 1
			И	МестныйКэш.Интеграция.ДоступныСерверныеНастройки() Тогда
			
		ПараметрыПриИзменении(Элемент);
		ПараметрыИнтеграции = Новый Структура("АдресСервера, СпособОбмена, СпособХраненияНастроек, ВремяОжиданияОтвета", МестныйКэш.СБИС.АдресСервера);
		ЗаполнитьЗначенияСвойств(ПараметрыИнтеграции, МестныйКэш.Парам);
		МестныйКэш.ГлавноеОкно.ПерезапуститьГлавноеОкно(ПараметрыИнтеграции, Ложь, ИзмениласьФормаНастроек);
		
	Иначе
		
		ТекстСообщения	= "Хранение настроек на сервере не доступно для выбранного способа обмена!";
		ТаймАут			= 60;
		ТекстЗаголовка	= "Невозможно изменить способ хранения настроек!";
		
		#Если ТолстыйКлиентОбычноеПриложение Тогда
			Предупреждение(ТекстСообщения, ТаймАут, ТекстЗаголовка);
		#Иначе
			ПоказатьПредупреждение(,ТекстСообщения, ТаймАут, ТекстЗаголовка);
		#КонецЕсли
		
		СпособХраненияНастроек	= 0;
		ПараметрыПриИзменении(Элемент);
	КонецЕсли;
	
	сбисОбновитьЭлементы_РазделНастроек();
	
КонецПроцедуры


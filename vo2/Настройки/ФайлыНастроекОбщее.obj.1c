&НаКлиенте
Функция	сбисОтключитьФайлыНастроек(ФайлыУдалить = Неопределено) Экспорт
	
	// Удаляет выбранные файлы настроек	
	Отказ			= Ложь;
	ПараметрыОтбора	= Новый Структура("Фл", Истина);
	
	Если ФайлыУдалить = Неопределено Тогда
		ФайлыУдалить = ТабУстановленныеИниФайлы.НайтиСтроки(ПараметрыОтбора);
	КонецЕсли;       
	
	Если Не ФайлыУдалить.Количество() Тогда
		ФайлыУдалить = ТабУстановленныеИниФайлы;
	КонецЕсли;
	
	СписокИниКОтключению = Новый СписокЗначений;
	Для Каждого ИниФайл Из ФайлыУдалить Цикл
		СписокИниКОтключению.Добавить(ИниФайл.ИмяФайла, "sbis3");
		Если	ИниФайл.ЕстьИзменения
			И	МестныйКэш.Парам.СпособХраненияНастроек = 0 Тогда
			СписокИниКОтключению.Добавить(СтрЗаменить(СтрЗаменить(ИниФайл.ИмяФайла, ".sbis3", ""), ".0", ""));
		КонецЕсли;							
	КонецЦикла;
	
	УстановленныеИни = МестныйКэш.ФормаНастроек.сбисОтключитьВыбранныеФайлыНастроек(СписокИниКОтключению, МестныйКэш, МестныйКэш.Парам.ИдентификаторНастроек, Отказ);
	
	Попытка
		СбисУстановитьНастройки(Новый Структура("ПринудительнаяПроверка, СписокИни", Истина, УстановленныеИни), МестныйКэш);
	Исключение
		ИнфоОбОшибке = ИнформацияОбОшибке();
		МестныйКэш.ГлавноеОкно.СбисСообщитьОбОшибке(МестныйКэш, МодульОбъектаКлиент().НовыйСбисИсключение(ИнфоОбОшибке, "ФайлыНастроекСервер.сбисОтключитьФайлыНастроек"), Новый Структура("ФормаВладелец", МестныйКэш.ФормаНастроекОбщее));		
	КонецПопытки;
	
	сбисОбновитьТаблицыИниФайлов(УстановленныеИни, Отказ); 
	
КонецФункции

